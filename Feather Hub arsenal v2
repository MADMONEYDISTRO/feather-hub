-- Function to create color preset button
local function CreateColorPreset(name, color1, color2, color3, parent, posY)
	local PresetBtn = Instance.new("TextButton")
	local PresetBtnCorner = Instance.new("UICorner")
	local ColorIndicator1 = Instance.new("Frame")
	local ColorIndicator2 = Instance.new("Frame")
	local ColorIndicator3 = Instance.new("Frame")
	local Indicator1Corner = Instance.new("UICorner")
	local Indicator2Corner = Instance.new("UICorner")
	local Indicator3Corner = Instance.new("UICorner")
	
	PresetBtn.Name = name .. "Preset"
	PresetBtn.Parent = parent
	PresetBtn.BackgroundColor3 = Color3.fromRGB(35, 35, 45)
	PresetBtn.BorderSizePixel = 0
	PresetBtn.Position = UDim2.new(0, 10, 0, posY)
	PresetBtn.Size = UDim2.new(1, -20, 0, 45)
	PresetBtn.Font = Enum.Font.Gotham
	PresetBtn.Text = "  " .. name
	PresetBtn.TextColor3 = Color3.fromRGB(200, 200, 220)
	PresetBtn.TextSize = 12
	PresetBtn.TextXAlignment = Enum.TextXAlignment.Left
	
	PresetBtnCorner.CornerRadius = UDim.new(0, 8)
	PresetBtnCorner.Parent = PresetBtn
	
	ColorIndicator1.Name = "Color1"
	ColorIndicator1.Parent = PresetBtn
	ColorIndicator1.BackgroundColor3 = color1
	ColorIndicator1.BorderSizePixel = 0
	ColorIndicator1.Position = UDim2.new(1, -95, 0.5, -10)
	ColorIndicator1.Size = UDim2.new(0, 20, 0, 20)
	
	Indicator1Corner.CornerRadius = UDim.new(0, 4)
	Indicator1Corner.Parent = ColorIndicator1
	
	ColorIndicator2.Name = "Color2"
	ColorIndicator2.Parent = PresetBtn
	ColorIndicator2.BackgroundColor3 = color2
	ColorIndicator2.BorderSizePixel = 0
	ColorIndicator2.Position = UDim2.new(1, -65, 0.5, -10)
	ColorIndicator2.Size = UDim2.new(0, 20, 0, 20)
	
	Indicator2Corner.CornerRadius = UDim.new(0, 4)
	Indicator2Corner.Parent = ColorIndicator2
	
	ColorIndicator3.Name = "Color3"
	ColorIndicator3.Parent = PresetBtn
	ColorIndicator3.BackgroundColor3 = color3
	ColorIndicator3.BorderSizePixel = 0
	ColorIndicator3.Position = UDim2.new(1, -35, 0.5, -10)
	ColorIndicator3.Size = UDim2.new(0, 20, 0, 20)
	
	Indicator3Corner.CornerRadius = UDim.new(0, 4)
	Indicator3Corner.Parent = ColorIndicator3
	
	return {Button = PresetBtn, Colors = {color1, color2, color3}}
end

