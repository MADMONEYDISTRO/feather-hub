-- Fade loop toggle
local running = true

-- Loop fade animation for "Preparing Script..."
task.spawn(function()
	while running do
		ts:Create(LoaderStatus, TweenInfo.new(0.6), {TextTransparency = 0}):Play()
		wait(0.6)
		ts:Create(LoaderStatus, TweenInfo.new(0.6), {TextTransparency = 0.6}):Play()
		wait(0.6)
	end
end)

-- Animate Loader
local loaderExpand = ts:Create(Loader, TweenInfo.new(0.8, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {Size = UDim2.new(0, 250, 0, 100)})
loaderExpand:Play()

wait(0.5)

local titleFade = ts:Create(LoaderTitle, TweenInfo.new(0.5), {TextTransparency = 0})
titleFade:Play()

local barFade = ts:Create(LoaderBar, TweenInfo.new(0.5), {BackgroundTransparency = 0})
barFade:Play()

wait(0.3)

local progressLoad = ts:Create(LoaderProgress, TweenInfo.new(2.5, Enum.EasingStyle.Quad), {Size = UDim2.new(1, 0, 1, 0)})
progressLoad:Play()

wait(2.8)

-- Stop fade loop
running = false

-- Fade everything out
local loaderFade = ts:Create(Loader, TweenInfo.new(0.5), {BackgroundTransparency = 1})
local strokeFade = ts:Create(LoaderStroke, TweenInfo.new(0.5), {Transparency = 1})
local titleFadeOut = ts:Create(LoaderTitle, TweenInfo.new(0.5), {TextTransparency = 1})
local statusFadeOut = ts:Create(LoaderStatus, TweenInfo.new(0.5), {TextTransparency = 1})
local barFadeOut = ts:Create(LoaderBar, TweenInfo.new(0.5), {BackgroundTransparency = 1})
local progressFadeOut = ts:Create(LoaderProgress, TweenInfo.new(0.5), {BackgroundTransparency = 1})

loaderFade:Play()
strokeFade:Play()
titleFadeOut:Play()
statusFadeOut:Play()
barFadeOut:Play()
progressFadeOut:Play()

wait(0.6)
Loader:Destroy()


