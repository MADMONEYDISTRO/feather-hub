    caseIcon.Parent = caseButton

    local caseText = Instance.new("TextLabel")
    caseText.Size = UDim2.new(1, -60, 1, 0)
    caseText.Position = UDim2.new(0, 50, 0, 0)
    caseText.BackgroundTransparency = 1
    caseText.Text = "INSTA OPEN CASE"
    caseText.TextColor3 = colors.text
    caseText.Font = Enum.Font.GothamBold
    caseText.TextSize = 16
    caseText.TextXAlignment = Enum.TextXAlignment.Left
    caseText.Parent = caseButton

    
    local statusPanel = Instance.new("Frame")
    statusPanel.Size = UDim2.new(1, 0, 0, 70)
    statusPanel.Position = UDim2.new(0, 0, 0, 124)
    statusPanel.BackgroundColor3 = colors.surface
    statusPanel.BackgroundTransparency = 0.3
    statusPanel.BorderSizePixel = 0
    statusPanel.Parent = content

    local statusCorner = Instance.new("UICorner")
    statusCorner.CornerRadius = UDim.new(0, 12)
    statusCorner.Parent = statusPanel

    local statusTitle = Instance.new("TextLabel")
    statusTitle.Size = UDim2.new(1, -20, 0, 20)
    statusTitle.Position = UDim2.new(0, 12, 0, 10)
    statusTitle.BackgroundTransparency = 1
    statusTitle.Text = "STATUS"
    statusTitle.TextColor3 = colors.textSecondary
    statusTitle.Font = Enum.Font.GothamBold
    statusTitle.TextSize = 11
    statusTitle.TextXAlignment = Enum.TextXAlignment.Left
    statusTitle.Parent = statusPanel

    local statusLabel = Instance.new("TextLabel")
    statusLabel.Size = UDim2.new(1, -20, 0, 28)
    statusLabel.Position = UDim2.new(0, 12, 0, 32)
    statusLabel.BackgroundTransparency = 1
    statusLabel.Text = "Ready to start..."
    statusLabel.TextColor3 = colors.text
    statusLabel.Font = Enum.Font.Gotham
    statusLabel.TextSize = 13
    statusLabel.TextXAlignment = Enum.TextXAlignment.Left
    statusLabel.TextWrapped = true
    statusLabel.Parent = statusPanel

   
    addButtonEffect(farmButton, Color3.fromRGB(109, 112, 251), Color3.fromRGB(89, 92, 231))
    addButtonEffect(caseButton, Color3.fromRGB(178, 95, 257), Color3.fromRGB(158, 75, 237))

    
    local function updateInput(input)
        local delta = input.Position - dragStart
        mainFrame.Position = UDim2.new(
            startPos.X.Scale,
            startPos.X.Offset + delta.X,
            startPos.Y.Scale,
            startPos.Y.Offset + delta.Y
        )
    end

    headerBar.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
            isDragging = true
            dragStart = input.Position
            startPos = mainFrame.Position

            input.Changed:Connect(function()
                if input.UserInputState == Enum.UserInputState.End then
                    isDragging = false
                end
            end)
        end
    end)

    headerBar.InputChanged:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
            dragInput = input
        end
    end)

    UserInputService.InputChanged:Connect(function(input)
        if input == dragInput and isDragging then
            updateInput(input)
        end
    end)

   
    local isFarming = false
    local lastTouchTime = 0

    local function toggleFarm()
        isFarming = not isFarming
        if isFarming then
            farmText.Text = "STOP FARM"
            farmIcon.Text = ""
            farmButton.BackgroundColor3 = colors.danger
            statusLabel.Text = "Preparing farm system..."

            spawn(function()
                local cashPart = workspace:FindFirstChild("Obbies") and 
                                 workspace.Obbies:FindFirstChild("9") and 
                                 workspace.Obbies["9"]:FindFirstChild("Folder") and 
                                 workspace.Obbies["9"].Folder:FindFirstChild("Cash") and 
                                 workspace.Obbies["9"].Folder.Cash:FindFirstChild("Collect")

                if not (cashPart and cashPart:IsA("BasePart")) then
                    statusLabel.Text = " Collection block not found!"
                    isFarming = false
                    farmText.Text = "FARM MONEY"
                    farmIcon.Text = ""
                    farmButton.BackgroundColor3 = colors.primary
                    return
                end

                local touchInterest = cashPart:FindFirstChild("TouchInterest")
                local char = localPlayer.Character or localPlayer.CharacterAdded:Wait()
                local hrp = char:WaitForChild("HumanoidRootPart", 5)
                
                if not hrp then
                    statusLabel.Text = "Character not ready!"
                    isFarming = false
                    return
                end

                hrp.CFrame = cashPart.CFrame * CFrame.new(0, 3, 0)
                statusLabel.Text = " Farming active..."

                local heartbeatConn
                heartbeatConn = RunService.Heartbeat:Connect(function()
                    if not isFarming then
                        heartbeatConn:Disconnect()
                        statusLabel.Text = "Farm stopped."
                        return
                    end

                    if tick() - lastTouchTime < 0.3 then return end
                    lastTouchTime = tick()

                    if touchInterest then
                        firetouchinterest(hrp, cashPart, 0)
                        firetouchinterest(hrp, cashPart, 1)
                    else
                        local origPos = hrp.CFrame
                        hrp.CFrame = cashPart.CFrame * CFrame.new(0, 2.5, 0)
                        wait(0.05)
                        hrp.CFrame = origPos
                    end
                end)
            end)
        else
            farmText.Text = "FARM MONEY"
            farmIcon.Text = ""
            farmButton.BackgroundColor3 = colors.primary
            statusLabel.Text = "Farm stopped."
        end
    end

    local function setAnimationSpeed()
        local settings = localPlayer:FindFirstChild("Settings")
        if settings then
            local animSpeed = settings:FindFirstChild("Animation Speed")
            if animSpeed and animSpeed:IsA("NumberValue") then
                animSpeed.Value = 1000
                statusLabel.Text = " Animation Speed set to 1000"
                spawn(function()
                    wait(2)
                    statusLabel.Text = isFarming and " Farming active..." or "Ready to start..."
                end)
            else
                statusLabel.Text = "Animation Speed not found!"
            end
        else
            statusLabel.Text = "Settings not found!"
        end
    end

    
    farmButton.MouseButton1Click:Connect(toggleFarm)
    caseButton.MouseButton1Click:Connect(setAnimationSpeed)
    minimizeBtn.MouseButton1Click:Connect(function()
        mainFrame.Visible = false
    end)
    closeBtn.MouseButton1Click:Connect(function()
        screenGui:Destroy()
        print(" UI removed from memory.")
    end)

    
    mainFrame.Size = UDim2.new(0, 0, 0, 0)
    mainFrame.Position = UDim2.new(0.5, 0, 0.5, 0)
    TweenService:Create(mainFrame, TweenInfo.new(0.5, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {
        Size = UDim2.new(0, 340, 0, 280),
        Position = UDim2.new(0.5, -170, 0.5, -140)
    }):Play()

    return mainFrame, screenGui
end


local guiFrame, guiContainer = createModernGUI()
print([[
Feather Hub Loaded!
 Free Edition
]])
