    minimizeBtn.TextColor3 = colors.textSecondary
    minimizeBtn.Font = Enum.Font.GothamBold
    minimizeBtn.TextSize = 20
    minimizeBtn.BackgroundTransparency = 1
    minimizeBtn.Parent = headerBar

    
    local closeBtn = Instance.new("TextButton")
    closeBtn.Size = UDim2.new(0, 32, 0, 32)
    closeBtn.Position = UDim2.new(1, -45, 0, 9)
    closeBtn.Text = "x"
    closeBtn.TextColor3 = colors.danger
    closeBtn.Font = Enum.Font.GothamBold
    closeBtn.TextSize = 18
    closeBtn.BackgroundTransparency = 1
    closeBtn.Parent = headerBar

   
    local content = Instance.new("Frame")
    content.Size = UDim2.new(1, -30, 1, -70)
    content.Position = UDim2.new(0, 15, 0, 60)
    content.BackgroundTransparency = 1
    content.Parent = mainFrame

   
    local farmButton = Instance.new("TextButton")
    farmButton.Size = UDim2.new(1, 0, 0, 52)
    farmButton.Position = UDim2.new(0, 0, 0, 0)
    farmButton.BackgroundColor3 = colors.primary
    farmButton.BorderSizePixel = 0
    farmButton.AutoButtonColor = false
    farmButton.Text = ""
    farmButton.Parent = content

    local farmCorner = Instance.new("UICorner")
    farmCorner.CornerRadius = UDim.new(0, 12)
    farmCorner.Parent = farmButton

    local farmIcon = Instance.new("TextLabel")
    farmIcon.Size = UDim2.new(0, 30, 1, 0)
    farmIcon.Position = UDim2.new(0, 15, 0, 0)
    farmIcon.BackgroundTransparency = 1
    farmIcon.Text = ""
    farmIcon.TextSize = 20
    farmIcon.Parent = farmButton

    local farmText = Instance.new("TextLabel")
    farmText.Size = UDim2.new(1, -60, 1, 0)
    farmText.Position = UDim2.new(0, 50, 0, 0)
    farmText.BackgroundTransparency = 1
    farmText.Text = "FARM MONEY"
    farmText.TextColor3 = colors.text
    farmText.Font = Enum.Font.GothamBold
    farmText.TextSize = 16
    farmText.TextXAlignment = Enum.TextXAlignment.Left
    farmText.Parent = farmButton

    
    local caseButton = Instance.new("TextButton")
    caseButton.Size = UDim2.new(1, 0, 0, 52)
    caseButton.Position = UDim2.new(0, 0, 0, 62)
    caseButton.BackgroundColor3 = colors.accent
    caseButton.BorderSizePixel = 0
    caseButton.AutoButtonColor = false
    caseButton.Text = ""
    caseButton.Parent = content

